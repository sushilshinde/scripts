
sudo curl -L https://yt-dl.org/downloads/latest/youtube-dl -o /usr/local/bin/youtube-dl
sudo chmod a+rx /usr/local/bin/youtube-dl

sudo curl -L https://raw.githubusercontent.com/sushilshinde/scripts/master/youtube-dl-batch -o /usr/local/bin/youtube-dl-batch 
sudo chmod a+rx /usr/local/bin/youtube-dl-batch
